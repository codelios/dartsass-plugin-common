{
    "name": "dartsass-plugin-common",
    "version": "0.10.1",
    "license": "MIT",
    "description": "Common Library code to compile sass files using Dart/JS compiler",
    "main": "dist/main.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codelios/dartsass-plugin-common.git"
    },
    "keywords": [
        "scss compiler plugin",
        "sass compiler plugin",
        "sass watcher",
        "scss watcher",
        "dartsass plugin",
        "sass",
        "scss",
        "css",
        "vsce",
        "vsce plugin",
        "vsce sass plugin",
        "vsce scss plugin"
    ],
    "scripts": {
        "sass": "sass",
        "prepublishOnly": "yarn prod",
        "clean": "rm -fr lib",
        "lint": "eslint src --ext ts && eslint __tests__ --ext ts",
        "fix": "eslint --fix . --ext .ts",
        "compile": "yarn clean && tsc",
        "dev": "rm -fr node_modules/fsevents && yarn compile && webpack --mode development",
        "prod": "rm -fr node_modules/fsevents && yarn compile && webpack --mode production",
        "testonly": "mocha --timeout 5000 -r ts-node/register __tests__/**/*.spec.ts",
        "test": "yarn dev && yarn testonly"
    },
    "dependencies": {
        "autoprefixer": "10.2.4",
        "browserslist": "^4.16.3",
        "chokidar": "3.5.1",
        "clean-css": "5.0.1",
        "fibers": "5.0.0",
        "postcss": "8.2.4",
        "sass": "^1.32.6"
    },
    "devDependencies": {
        "@types/chai": "4.2.14",
        "@types/clean-css": "4.2.3",
        "@types/fibers": "3.1.0",
        "@types/mocha": "8.2.0",
        "@types/node": "14.14.22",
        "@types/sass": "1.16.0",
        "@typescript-eslint/eslint-plugin": "4.14.2",
        "@typescript-eslint/parser": "4.14.2",
        "chai": "4.2.0",
        "eslint": "7.19.0",
        "eslint-config-prettier": "7.2.0",
        "eslint-plugin-prettier": "3.3.1",
        "minimist": "1.2.5",
        "mocha": "8.2.1",
        "prettier": "2.2.1",
        "ts-loader": "8.0.14",
        "ts-node": "9.1.1",
        "typescript": "4.1.3",
        "webpack": "5.20.1",
        "webpack-cli": "4.5.0"
    }
}
