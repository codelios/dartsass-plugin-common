{
    "name": "dartsass-plugin-common",
    "version": "0.8.0",
    "license": "MIT",
    "description": "Common Library code to compile sass files using Dart/JS compiler",
    "main": "dist/main.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codelios/dartsass-plugin-common.git"
    },
    "keywords": [
        "scss compiler plugin",
        "sass compiler plugin",
        "sass watcher",
        "scss watcher",
        "dartsass plugin",
        "sass",
        "scss",
        "css",
        "vsce",
        "vsce plugin",
        "vsce sass plugin",
        "vsce scss plugin"
    ],
    "scripts": {
        "sass": "sass",
        "prepublishOnly": "yarn prod",
        "clean": "rm -fr lib",
        "lint": "eslint src --ext ts && eslint __tests__ --ext ts",
        "fix": "eslint --fix . --ext .ts",
        "compile": "yarn clean && tsc",
        "dev": "rm -fr node_modules/fsevents && yarn compile && webpack --mode development",
        "prod": "rm -fr node_modules/fsevents && yarn compile && webpack --mode production",
        "testonly": "mocha --timeout 5000 -r ts-node/register __tests__/**/*.spec.ts",
        "test": "yarn dev && yarn testonly"
    },
    "dependencies": {
        "autoprefixer": "10.0.1",
        "browserslist": "4.14.5",
        "chokidar": "3.4.2",
        "clean-css": "4.2.3",
        "fibers": "5.0.0",
        "postcss": "8.1.1",
        "sass": "^1.27.0"
    },
    "devDependencies": {
        "@types/autoprefixer": "9.7.2",
        "@types/chai": "4.2.13",
        "@types/clean-css": "4.2.2",
        "@types/fibers": "3.1.0",
        "@types/mocha": "8.0.3",
        "@types/node": "14.11.2",
        "@types/sass": "1.16.0",
        "@typescript-eslint/eslint-plugin": "4.4.0",
        "@typescript-eslint/parser": "4.4.0",
        "chai": "4.2.0",
        "eslint": "7.11.0",
        "eslint-config-prettier": "6.12.0",
        "eslint-plugin-prettier": "3.1.4",
        "minimist": ">=1.2.3",
        "mocha": "8.1.3",
        "prettier": "2.1.2",
        "ts-loader": "8.0.4",
        "ts-node": "9.0.0",
        "typescript": "4.0.3",
        "webpack": "5.0.0",
        "webpack-cli": "4.0.0"
    }
}
