{
    "name": "dartsass-plugin-common",
    "version": "0.1.5",
    "license": "MIT",
    "description": "Common Library code to compile sass files using Dart/JS compiler",
    "main": "dist/main.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/malvahq/dartsass-plugin-common.git"
    },
    "scripts": {
        "clean": "rm -fr lib",
        "compile": "npm run clean && tsc",
        "lint": "tslint -p tsconfig.json",
        "dev": "npm run compile && webpack --mode development",
        "prod": "npm run compile && webpack --mode production"
    },
    "dependencies": {
        "sass": "1.23.0",
        "@types/sass": "1.16.0",
        "node-sass-package-importer": "5.3.2",
        "fibers": "4.0.1",
        "@types/fibers": "0.0.30",
        "autoprefixer": "9.6.4",
        "@types/autoprefixer": "9.6.1",
        "postcss": "7.0.18",
        "browserslist": "4.7.0",
        "typescript": "3.5.3"
    },
    "devDependencies": {
        "ts-node": "8.4.1",
        "webpack": "4.41.0",
        "webpack-cli": "3.3.9",
        "eslint": "6.5.1",
        "ts-loader": "6.2.0",
        "tslint": "5.20.0"
    }
}
