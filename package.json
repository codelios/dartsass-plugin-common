{
    "name": "dartsass-plugin-common",
    "version": "0.7.65",
    "license": "MIT",
    "description": "Common Library code to compile sass files using Dart/JS compiler",
    "main": "dist/main.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codelios/dartsass-plugin-common.git"
    },
    "keywords": [
        "scss compiler plugin",
        "sass compiler plugin",
        "sass watcher",
        "scss watcher",
        "dartsass plugin",
        "sass",
        "scss",
        "css",
        "vsce",
        "vsce plugin",
        "vsce sass plugin",
        "vsce scss plugin"
    ],
    "scripts": {
        "sass": "sass",
        "prepublishOnly": "npm run prod",
        "clean": "rm -fr lib",
        "compile": "npm run clean && tsc",
        "lint": "eslint src --ext ts && eslint __tests__ --ext ts",
        "dev": "rm -fr node_modules/fsevents && npm run compile && webpack --mode development",
        "prod": "rm -fr node_modules/fsevents && npm run compile && webpack --mode production",
        "test": "npm run dev && mocha --timeout 5000 -r ts-node/register __tests__/**/*.spec.ts"
    },
    "dependencies": {
        "sass": "1.26.3",
        "fibers": "4.0.2",
        "autoprefixer": "9.7.6",
        "postcss": "7.0.27",
        "browserslist": "4.11.1",
        "typescript": "3.8.3",
        "chokidar": "3.3.1",
        "clean-css": "4.2.3"
    },
    "devDependencies": {
        "@types/sass": "1.16.0",
        "@types/fibers": "3.1.0",
        "@types/autoprefixer": "9.7.2",
        "@types/clean-css": "4.2.1",
        "@types/node": "13.9.8",
        "ts-node": "8.8.2",
        "mocha": "7.1.1",
        "@types/mocha": "7.0.2",
        "chai": "4.2.0",
        "@types/chai": "4.2.11",
        "webpack": "4.42.1",
        "webpack-cli": "3.3.11",
        "ts-loader": "6.2.2",
        "minimist": ">=1.2.3",
        "eslint": "6.8.0",
        "@typescript-eslint/parser": "2.27.0",
        "@typescript-eslint/eslint-plugin": "2.27.0"
    }
}
