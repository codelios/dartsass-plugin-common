{
    "name": "dartsass-plugin-common",
    "version": "0.7.67",
    "license": "MIT",
    "description": "Common Library code to compile sass files using Dart/JS compiler",
    "main": "dist/main.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codelios/dartsass-plugin-common.git"
    },
    "keywords": [
        "scss compiler plugin",
        "sass compiler plugin",
        "sass watcher",
        "scss watcher",
        "dartsass plugin",
        "sass",
        "scss",
        "css",
        "vsce",
        "vsce plugin",
        "vsce sass plugin",
        "vsce scss plugin"
    ],
    "scripts": {
        "sass": "sass",
        "prepublishOnly": "yarn prod",
        "clean": "rm -fr lib",
        "lint": "eslint src --ext ts && eslint __tests__ --ext ts",
        "fix": "eslint --fix . --ext .ts",
        "compile": "yarn clean && tsc",
        "dev": "rm -fr node_modules/fsevents && yarn compile && webpack --mode development",
        "prod": "rm -fr node_modules/fsevents && yarn compile && webpack --mode production",
        "test": "yarn dev && mocha --timeout 5000 -r ts-node/register __tests__/**/*.spec.ts"
    },
    "dependencies": {
        "sass": "1.26.10",
        "fibers": "5.0.0",
        "autoprefixer": "9.8.5",
        "postcss": "7.0.32",
        "browserslist": "4.13.0",
        "chokidar": "3.4.1",
        "clean-css": "4.2.3"
    },
    "devDependencies": {
        "typescript": "3.9.7",
        "@types/sass": "1.16.0",
        "@types/fibers": "3.1.0",
        "@types/autoprefixer": "9.7.2",
        "@types/clean-css": "4.2.2",
        "@types/node": "13.9.8",
        "ts-node": "8.10.2",
        "mocha": "8.0.1",
        "@types/mocha": "8.0.0",
        "chai": "4.2.0",
        "@types/chai": "4.2.11",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.12",
        "ts-loader": "8.0.1",
        "prettier": "2.0.5",
        "minimist": ">=1.2.3",
        "eslint": "7.5.0",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-prettier": "3.1.4",
        "@typescript-eslint/parser": "3.6.1",
        "@typescript-eslint/eslint-plugin": "3.6.1"
    }
}
