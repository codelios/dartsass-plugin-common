{
    "name": "dartsass-plugin-common",
    "version": "0.4.1",
    "license": "MIT",
    "description": "Common Library code to compile sass files using Dart/JS compiler",
    "main": "dist/main.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/malvahq/dartsass-plugin-common.git"
    },
    "scripts": {
        "prepublishOnly": "npm run prod",
        "clean": "rm -fr lib",
        "compile": "npm run clean && tsc",
        "dev": "npm run compile && webpack --mode development",
        "prod": "npm run compile && webpack --mode production",
        "test": "npm run dev && mocha --timeout 5000 -r ts-node/register __tests__/**/*.spec.ts"
    },
    "dependencies": {
        "sass": "1.24.0",
        "@types/sass": "1.16.0",
        "node-sass-package-importer": "5.3.2",
        "fibers": "4.0.2",
        "@types/fibers": "3.1.0",
        "autoprefixer": "9.7.3",
        "@types/autoprefixer": "9.6.1",
        "postcss": "7.0.25",
        "browserslist": "4.8.2",
        "@types/mkdirp": "0.5.2",
        "mkdirp": "0.5.1",
        "typescript": "3.6.4"
    },
    "devDependencies": {
        "@types/node": "12.12.21",
        "ts-node": "8.5.4",
        "mocha": "6.2.2",
        "@types/mocha": "5.2.7",
        "chai": "4.2.0",
        "@types/chai": "4.2.7",
        "webpack": "4.41.4",
        "webpack-cli": "3.3.10",
        "ts-loader": "6.2.1"
    }
}
